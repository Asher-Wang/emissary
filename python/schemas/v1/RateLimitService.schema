{
    "$schema": "http://json-schema.org/schema#",
    "id": "https://getambassador.io/schemas/mapping.json",
    "type": "object",
    "properties": {
        "apiVersion": {
            "enum": [
                "getambassador.io/v1"
            ]
        },
        "generation": {
            "type": "integer"
        },
        "kind": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "namespace": {
            "type": "string"
        },
        "metadata_labels": {
            "type": "object",
            "additionalProperties": {
                "oneOf": [
                    {
                        "type": "string"
                    },
                    {
                        "type": "boolean"
                    }
                ]
            }
        },
        "ambassador_id": {
            "oneOf": [
                {
                    "type": "string"
                },
                {
                    "type": "array"
                }
            ],
            "items": {
                "type": "string"
            }
        },
        "service": {
            "type": "string"
        },
        "timeout_ms": {
            "type": "integer"
        },
        "domain": {
            "type": "string"
        },
        "tls": {
            "oneOf": [
                {
                    "type": "string"
                },
                {
                    "type": "boolean"
                }
            ]
        }
    },
    "required": [
        "apiVersion",
        "kind",
        "name",
        "service"
    ],
    "additionalProperties": false
}
